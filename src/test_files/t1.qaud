fac:
	EQU	n, 0, ..@l1
	SUB	n, 1, &1
	PUSH	-, -, &1
	CALL	fac, -, &2
	MUL	&2, n, &3
	SRET	&3, -, fac
	JMP	-, -, ..@l2
..@l1:
	SRET	1, -, fac
..@l2:
	FIN	-, -, -
main:
	ASS	0, -, vn
..@l3:
	GTT	vn, 4, ..@l4
	PUSH	-, -, vn
	CALL	fac, -, &4
	WRI	-, -, J_&4
	INC	-, -, vn
	JMP	-, -, ..@l3
..@l4:
	FIN	-, -, -

